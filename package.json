{
  "name": "stocktradingv2",
  "version": "1.0.0",
  "description": "Stock Trading Website",
  "scripts": {
    "build:css:main": "tailwindcss -i ./assets/css/main.css -o ./static/css/main.css --config ./tailwind.config.js",
    "build:css:learn": "tailwindcss -i ./assets/css/learn.css -o ./static/css/learn.css --config ./tailwind.learn.config.js",
    "build:css": "npm run build:css:main && npm run build:css:learn",
    "dev": "hugo server -D"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.3.5"
  }
}
