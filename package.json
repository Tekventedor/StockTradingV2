{
  "name": "stocktradingv2",
  "version": "1.0.0",
  "description": "Stock Trading Website",
  "scripts": {
    "build:css:v3": "postcss assets/css/main-v3.css -o static/css/v3.css -c postcss/v3.config.js",
    "build:css:v4": "postcss assets/css/main-v4.css -o static/css/v4.css -c postcss/v4.config.js",
    "build:css": "npm run build:css:v3 && npm run build:css:v4",
    "dev": "hugo server -D"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-v3": "npm:tailwindcss@^3.3.5"
  }
}
